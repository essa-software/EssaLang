func main() {
    let file = File::open("../../tests/stdlib/file_read_to_buf.esl");
    while (true) {
        let buf = file.read_to_buf(64);
        if (buf.size() == 0) {
            print("EOF!\n");
            break;
        }
        for (let i of 0..buf.size()) {
            print("{} ", buf.get(i));
        }
        print("\n");
    }
}
